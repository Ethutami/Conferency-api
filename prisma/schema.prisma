generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model categories {
  id   Int     @id @default(autoincrement())
  icon String? @db.VarChar(255)
  name String  @db.VarChar(255)
}
